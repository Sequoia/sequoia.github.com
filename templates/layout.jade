- var thisYear = (new Date()).getFullYear();
doctype html
html(lang='en')
  include ./includes/head.jade
  body
    #wrap

      header(role='banner')
        h1 Sequoia McDowell
        h2 Programmer, Educator
        include ./includes/navigation.jade

      aside.announce.row
        h2.three.columns News
