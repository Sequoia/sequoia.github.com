{
  "name": "sequoia.github.com",
  "version": "0.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "blog" : "node . && npm run blog-copy-imgs && npm run blog-copy-assets",
    "watch": "npm run bbl-watch & npm run blog-watch-assets",
    "push" : "git subtree push --prefix out origin master",
    "build": "npm run bbl-build",

    "bbl-build": "babel src --out-dir dist",
    "bbl-watch": "npm run bbl-build -- --watch",
    "postinstall": "npm run build",
    "blog-copy-assets": "cp -r site_assets/* out/",
    "blog-watch-assets": "watch --wait=1 'npm run blog-copy-assets' site_assets",
    "blog-copy-imgs": "mkdirp out/img && cp -r _content/img/* out/img/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sequoia/sequoia.github.com.git"
  },
