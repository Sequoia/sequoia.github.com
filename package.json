  "bugs": {
    "url": "https://github.com/Sequoia/sequoia.github.com/issues"
  },
  "homepage": "https://sequoia.makes.software",
  "dependencies": {
    "babel-core": "^6.3.21",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "bluebird": "^3.1.1",
    "front-matter": "^2.0.1",
    "highlight.js": "^9.0.0",
    "jade": "^1.11.0",
    "marked": "^0.3.5",
    "mkdirp": "^0.5.1",
    "months": "^1.0.1",
    "node-dir": "^0.1.11",
    "ramda": "^0.19.0",
    "root-path": "^0.2.0",
    "string": "^3.3.1",
    "watch": "^0.17.1"
  }
}
